{
    "htmlWhitespaceSensitivity": "ignore",
    "embeddedLanguageFormatting": "off",
    "goTemplateBracketSpacing": true,
    "plugins": [
        "prettier-plugin-go-template"
    ],
    "overrides": [
        {
            "files": [
                "*.html"
            ],
            "options": {
                "parser": "go-template"
            }
        }
    ]
}